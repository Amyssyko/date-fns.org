{
  "functions": {
    "source": "build/functions",
    "predeploy": ["yarn build:server"],
    "runtime": "nodejs14"
  },
  "hosting": {
    "public": "build/client",
    "predeploy": ["yarn build:client"],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ]
  }
}
