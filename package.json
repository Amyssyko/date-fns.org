{
  "name": "date-fns.org",
  "version": "3.0.0",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "lint": "tslint './src/**/*.{ts,tsx}' --project tsconfig.json -c config/tslint.yaml",
    "format": "prettier --write",
    "build": "yarn build:client && yarn build:server",
    "build:client": "rm -rf dist && webpack --config ./config/webpack.config.client.js && ./scripts/build/robots.sh",
    "build:server": "rm -rf build && webpack --config ./config/webpack.config.server.js && ./scripts/build/packageJSON.js",
    "deploy": "firebase deploy",
    "logs": "firebase functions:log",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/**/test.ts'",
    "start": "env NODE_ENV=development webpack serve --config ./config/webpack.config.devserver.js"
  },
  "dependencies": {
    "@date-fns/date-fns-db": "^0.0.14",
    "@sentry/browser": "^5.30.0",
    "@sentry/tracing": "^5.30.0",
    "@switcher/preact": "^1.1.2",
    "@types/lodash": "^4.14.167",
    "@types/remarkable": "^2.0.0",
    "@typesaurus/preact": "^4.0.0",
    "body-parser": "^1.19.0",
    "codemirror": "^5.57.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "firebase": "^8.2.2",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "isomorphic-fetch": "^2.2.1",
    "js-fns": "^2.5.1",
    "json-bond": "^0.1.0",
    "lodash": "^4.17.20",
    "preact": "^10.4.8",
    "preact-render-to-string": "^5.1.10",
    "remarkable": "^2.0.1",
    "reset.css": "^2.0.2",
    "typesaurus": "^7.1.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@nyancss/css-modules-loader": "^1.1.0",
    "@nyancss/react": "^1.3.0",
    "@types/codemirror": "^0.0.98",
    "@types/cors": "^2.8.7",
    "@types/enzyme": "^3.10.8",
    "@types/mocha": "^8.2.0",
    "@types/power-assert": "^1.5.3",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^3.0.0",
    "ejs-webpack-loader": "^2.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.3",
    "file-loader": "^6.1.0",
    "firebase-tools": "^9.2.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "mocha": "^8.2.1",
    "power-assert": "^1.6.1",
    "prettier": "2.1.1",
    "raw-loader": "^4.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslint": "~6.1.3",
    "typescript": "^4.0.2",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.1",
    "webpack-node-externals": "^2.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "yarn format"
    ],
    "*.{ts,tsx}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
