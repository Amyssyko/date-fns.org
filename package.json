{
  "name": "date-fns.org",
  "version": "3.0.0",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "lint": "tslint './src/**/*.{ts,tsx}' --project tsconfig.json -c tslint.yaml",
    "format": "prettier --write",
    "build": "yarn build:web && yarn build:functions",
    "build:web": "rm -rf build/web && webpack --config webpack.config.web.ts && ./scripts/build/robots.sh",
    "build:functions": "rm -rf build/functions && webpack --config webpack.config.functions.ts && ./scripts/build/packageJSON.js",
    "deploy": "firebase deploy",
    "logs": "firebase functions:log",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/**/test.ts'",
    "start": "env NODE_ENV=development ts-node -r tsconfig-paths/register devServer.ts"
  },
  "dependencies": {
    "@date-fns/date-fns-db": "^0.0.14",
    "@nyancss/css-modules-loader": "^1.1.0",
    "@nyancss/react": "^1.3.0",
    "@sentry/browser": "^5.30.0",
    "@sentry/tracing": "^5.30.0",
    "@switcher/preact": "^1.1.2",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/cors": "^2.8.7",
    "@types/lodash": "^4.14.167",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.14.32",
    "@types/prismjs": "^1.16.3",
    "@types/remarkable": "^2.0.0",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-node-externals": "^2.5.0",
    "@typesaurus/preact": "^4.0.0",
    "body-parser": "^1.19.0",
    "copy-webpack-plugin": "^7.0.0",
    "cors": "^2.8.5",
    "css-loader": "^3.0.0",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "file-loader": "^6.1.0",
    "firebase": "^8.2.2",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "firebase-tools": "^9.2.0",
    "html-webpack-plugin": "^5.2.0",
    "isomorphic-fetch": "^2.2.1",
    "json-bond": "^0.1.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.9",
    "preact": "^10.4.8",
    "preact-render-to-string": "^5.1.10",
    "prismjs": "^1.23.0",
    "remarkable": "^2.0.1",
    "reset.css": "^2.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typesaurus": "^7.1.0",
    "typescript": "^4.0.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.1",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/mocha": "^8.2.0",
    "@types/power-assert": "^1.5.3",
    "@types/webpack-dev-middleware": "^4.1.0",
    "@types/webpack-hot-middleware": "^2.25.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "mocha": "^8.2.1",
    "power-assert": "^1.6.1",
    "prettier": "2.1.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~6.1.3",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "yarn": "^1.22.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "yarn format"
    ],
    "*.{ts,tsx}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
